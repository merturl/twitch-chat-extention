{
  "name": "twitch chat monitor",
  "description": "Testing a Twitch login system.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
